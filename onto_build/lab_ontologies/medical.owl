<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/medical.owl"
         xmlns="http://example.org/medical.owl#">

<owl:Ontology rdf:about="http://example.org/medical.owl"/>

<owl:ObjectProperty rdf:about="#has_symptom">
  <rdfs:domain rdf:resource="#Disease"/>
  <rdfs:range rdf:resource="#Symptom"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#Disease">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Symptom">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Cancer">
  <rdfs:subClassOf rdf:resource="#Disease"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Disease"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_symptom"/>
          <owl:someValuesFrom rdf:resource="#Symptom"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:NamedIndividual rdf:about="#lung_cancer">
  <rdf:type rdf:resource="#Cancer"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#cough">
  <rdf:type rdf:resource="#Symptom"/>
</owl:NamedIndividual>


</rdf:RDF>
