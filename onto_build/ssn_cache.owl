  <owl:Class rdf:about="http://www.w3.org/ns/ssn/Latency">
    <rdfs:label xml:lang="en">Latency</rdfs:label>
    <rdfs:comment xml:lang="en">The time between a request for an observation (resp. actuation) and the sensor providing a result (resp. the actuator operating the actuation).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasSystemCapability">
    <rdfs:label xml:lang="en">has system capability</rdfs:label>
    <rdfs:comment xml:lang="en">Relation from a System to a SystemCapability describing the capabilities of the system.</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasProperty">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
        <rdfs:comment xml:lang="en">Relation between an entity and a Property of that entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
        <rdfs:label xml:lang="en">has property</rdfs:label>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasSystemCapability"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/identifier"/>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/MeasurementRange">
    <rdfs:label xml:lang="en">Measurement Range</rdfs:label>
    <rdfs:comment xml:lang="en">The set of values that the sensor can return as the result of an observation under the defined conditions with the defined system properties.  If no conditions are specified or the conditions do not specify a range for the observed qualities, the measurement range is to be taken as the condition for the observed qualities.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/isProxyFor">
    <skos:definition xml:lang="en">A relation from a Stimulus to the Property that the Stimulus is serving as a proxy for.</skos:definition>
    <rdfs:comment xml:lang="en">A relation from a Stimulus to the Property that the Stimulus is serving as a proxy for.</rdfs:comment>
    <skos:example xml:lang="en">For example, the expansion of quicksilver is a stimulus that serves as a proxy for temperature. An increase or decrease in the velocity of spinning cups on a wind sensor is serving as a proxy for wind speed.</skos:example>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:label xml:lang="en">is proxy for</rdfs:label>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/rights"/>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#isDefinedBy"/>
  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/madeActuation">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/implementedBy">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/implements">
        <skos:definition xml:lang="en">Relation between an entity that implements a Procedure in some executable way and the Procedure (an algorithm, procedure or method).</skos:definition>
        <rdfs:comment xml:lang="en">Relation between an entity that implements a Procedure in some executable way and the Procedure (an algorithm, procedure or method).</rdfs:comment>
        <skos:example xml:lang="en">For example, the relationship between a sensor and the scientific measuring Procedure via which it senses.</skos:example>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
        <rdfs:label xml:lang="en">implements</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>
    <skos:definition xml:lang="en">Relation between a Procedure (an algorithm, procedure or method) and an entity that implements that Procedure in some executable way.</skos:definition>
    <rdfs:comment xml:lang="en">Relation between a Procedure (an algorithm, procedure or method) and an entity that implements that Procedure in some executable way.</rdfs:comment>
    <skos:example xml:lang="en">For example, the relationship between a scientific measuring Procedure and a sensor that senses via that Procedure.</skos:example>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:label xml:lang="en">implemented by</rdfs:label>
  </owl:ObjectProperty>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/ActuationRange">
    <rdfs:label xml:lang="en">Actuation Range</rdfs:label>
    <rdfs:comment xml:lang="en">The range of quality values that can be the result of an actuation under the defined conditions with the defined system properties.  If no conditions are specified or the conditions do not specify a range for the actuated qualities, the actuation range is to be taken as the condition for the actuatable qualities.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/isObservedBy">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/source"/>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/DetectionLimit">
    <rdfs:label xml:lang="en">Detection Limit</rdfs:label>
    <rdfs:comment xml:lang="en">An observed value for which the probability of falsely claiming the absence of a component in a material is beta, given a probability alpha of falsely claiming its presence.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/madeSampling">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/modified"/>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/MaintenanceSchedule">
    <rdfs:label xml:lang="en">Maintenance Schedule</rdfs:label>
    <rdfs:comment xml:lang="en">Schedule of maintenance for a system in the specified conditions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/OperatingProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/ofFeature">
    <rdfs:comment>A relation between some aspect of a sensing entity and a feature. For example, from a Sensor to the FeatureOfInterest it can observe properties of, or from a Deployment to the FeatureOfInterest it was installed to observe. Also from a MeasurementCapability to the FeatureOfInterest the capability is described for (used in conjunction with forProperty).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:label xml:lang="en">of feature</rdfs:label>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/madeObservation">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/BatteryLifetime">
    <rdfs:label xml:lang="en">Battery Lifetime</rdfs:label>
    <rdfs:comment xml:lang="en">Total useful life of a battery.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SurvivalProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/isSampleOf">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/isActedOnBy">
