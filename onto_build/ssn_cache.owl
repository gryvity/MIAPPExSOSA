  <owl:Class rdf:about="http://www.w3.org/ns/ssn/Sensitivity">
    <rdfs:label xml:lang="en">Sensitivity</rdfs:label>
    <rdfs:comment xml:lang="en">Sensitivity is the quotient of the change in a result of sensor and the corresponding change in a value of a quality being observed.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/Repeatability">
    <rdfs:label xml:lang="en">Repeatability</rdfs:label>
    <rdfs:comment xml:lang="en">The closeness of agreement between replicate actuations for an unchanged or similar command: i.e., a measure of an actuator's ability to consistently reproduce an actuation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/ssn/Precision">
    <rdfs:label xml:lang="en">Precision</rdfs:label>
    <rdfs:comment xml:lang="en">The closeness of agreement between replicate observations on an unchanged or similar quality value: i.e., a measure of a sensor's ability to consistently reproduce an observation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/SystemProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>
