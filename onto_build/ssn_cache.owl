  <owl:Class rdf:about="http://www.w3.org/ns/ssn/Deployment">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/DeploymentRelatedProcedure"/>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n8bc0bc5018b64ab485e5ed6023c297d4b1">
